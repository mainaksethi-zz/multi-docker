{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
	{
		"name": "client",
		"image": "sethimainak/multi-client",
		"hostname": "client",
		"essential": false
	},
	{
		"name": "server",
		"image": "sethimainak/multi-server",
		"hostname": "api",
		"essential": false
	},
	{
		"name": "worker",
		"image": "sethimainak/multi-worker",
		"hostname": "worker",
		"essential": false
	},
	{
		"name": "nginx",
		"image": "sethimainak/multi-nginx",
		"hostname": "nginx",
		"essential": true,
		"portMappings": [
		{
			"hostPort": 80,
			"containerPort": 80
		}],
		"links": ["server", "client"]
	}]
}